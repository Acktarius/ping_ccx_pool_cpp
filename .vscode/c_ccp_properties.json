{
    "configurations": [
        {
            "name": "Linux",
            "includePath": [
                "${workspaceFolder}/**",
                "/usr/local/include/wx-3.2",
                "/usr/local/lib/wx/include/gtk3-unicode-3.2"
            ],
            "defines": [
                "wxUSE_GUI=1",
                "__WXGTK__"
            ],
            "compilerPath": "/usr/bin/g++",
            "cStandard": "c11",
            "cppStandard": "c++17",
            "intelliSenseMode": "gcc-x64",
            "compilerArgs": [
                "`wx-config --cxxflags`"
            ]
        }
    ],
    "version": 4
}
